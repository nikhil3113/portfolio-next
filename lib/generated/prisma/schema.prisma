generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Contact {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  name    String
  email   String
  message String
}
